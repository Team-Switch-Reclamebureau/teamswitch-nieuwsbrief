		<!-- socials -->
		<mj-section padding-bottom="0" background-color="#fff">
			<mj-column>
				<mj-text>
					Volg ons op sociale media voor meer interessante
					marketing-tips die je kunt delen met je collega’s!
				</mj-text>
			</mj-column>
		</mj-section>
		<mj-section padding-top="0" padding-bottom="0" background-color="#fff">
			<mj-column width="50px">
				<mj-image
					src="https://files.teamswitch.dev/newsletter/teamswitch/img/social/switch-facebook.png"
					href="https://www.facebook.com/TeamSwitch.nl"
					alt=""
				/>
			</mj-column>
			<mj-column width="20px"></mj-column>
			<mj-column width="50px">
